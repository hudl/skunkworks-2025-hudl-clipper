{
  "name": "Hudl Clipper Tool",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "Chrome extension to clip images from websites for Fastscout.",
  "icons": {
    "32": "icon32.png",
    "64": "icon64.png",
    "128": "icon128.png",
    "256": "icon256.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon32.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "debugger",
    "activeTab",
    "tabs"
  ]
}
